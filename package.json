{
  "name": "9m88",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "watch": {
    "tmpl": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "mustache"
      ]
    }
  },
  "scripts": {
    "tmpl": "mustache src/data.json src/index.mustache -p ./src/logo.mustache > src/index.html",
    "watch:tmpl": "npm-watch",
    "watch:server": "parcel src/index.html",
    "build": "parcel build src/index.html --out-dir ./docs --public-url /9m88"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "mustache": "^3.1.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "sass": "^1.23.0",
    "tabletop": "^1.6.2"
  }
}
